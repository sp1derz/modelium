{
  "dashboard": {
    "title": "Forge - System Overview",
    "tags": ["forge", "overview"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Model Ingestion Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(forge_models_ingested_total[5m])",
            "legendFormat": "{{ namespace }}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Conversion Success Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(forge_conversions_success_total[5m]) / rate(forge_conversions_total[5m])",
            "legendFormat": "Success Rate"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Active Conversions",
        "type": "stat",
        "targets": [
          {
            "expr": "forge_active_conversions",
            "legendFormat": "Active"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Deployed Models",
        "type": "stat",
        "targets": [
          {
            "expr": "count(kube_pod_labels{label_serving_kserve_io_inferenceservice!=\"\"})",
            "legendFormat": "Deployed"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8}
      },
      {
        "id": 5,
        "title": "Average Conversion Time",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(forge_conversion_duration_seconds_sum[5m]) / rate(forge_conversion_duration_seconds_count[5m])",
            "legendFormat": "Avg Time (s)"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 6,
        "title": "Triton Inference Throughput",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(nv_inference_request_success[5m])",
            "legendFormat": "{{ model_name }}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 7,
        "title": "Triton Inference Latency (P95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(nv_inference_request_duration_us_bucket[5m]))",
            "legendFormat": "{{ model_name }}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 8,
        "title": "GPU Utilization",
        "type": "graph",
        "targets": [
          {
            "expr": "DCGM_FI_DEV_GPU_UTIL",
            "legendFormat": "{{ gpu }} - {{ node }}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 9,
        "title": "GPU Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "DCGM_FI_DEV_FB_USED / DCGM_FI_DEV_FB_FREE * 100",
            "legendFormat": "{{ gpu }} - {{ node }}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      }
    ],
    "refresh": "30s",
    "schemaVersion": 27,
    "style": "dark",
    "time": {
      "from": "now-6h",
      "to": "now"
    }
  }
}

